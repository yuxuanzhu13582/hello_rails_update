<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Context:</strong>
    <%= post.context %>
  </p>

  <p>
    <strong>Is available:</strong>
    <%= post.is_available %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user.name %>
  </p>

</div>
